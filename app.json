{
  "expo": {
    "name": "Medicamentar",
    "slug": "Medicamentar",
    "scheme": "Medicmentar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.collab.Medicamentar"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Medicamentar"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-notifications"
    ],
    "notification": {
      "icon": "./src/assets/logo_notification.png",
      "color:": "#ffffff"
    },
    "fonts": [
      {
        "fonts": [
          "./src/assets/fonts/armata-regular-400.ttf"
        ]
      }
    ]
  }
}
